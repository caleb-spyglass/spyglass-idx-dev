        <script>
        document.addEventListener('DOMContentLoaded', function() {
            // Track all contact forms with class 'followupboss-track' or specific IDs
            // Exclude forms that have their own AJAX handlers (like spyglass-contact-form and contact-form-footer)
            const contactForms = document.querySelectorAll('form[id*="contact"]:not(#spyglass-contact-form):not(#contact-form-footer), form.followupboss-track, form#contact-form-header');
            
            contactForms.forEach(function(form) {
                form.addEventListener('submit', function(e) {
                    e.preventDefault();
                    
                    // Get form data
                    const formData = new FormData(form);
                    const firstName = formData.get('first_name') || '';
                    const lastName = formData.get('last_name') || '';
                    const email = formData.get('email') || '';
                    const phone = formData.get('phone_number') || '';
                    const message = formData.get('message') || '';
                    
                    // Determine source based on form ID or class
                    let source = 'Contact Form';
                    if (form.id === 'contact-form-header') {
                        source = 'Contact Form Header';
                    } else if (form.closest('footer')) {
                        source = 'Contact Form Footer';
                    } else if (form.classList.contains('followupboss-track')) {
                        source = form.dataset.source || 'Contact Form';
                    }
                    
                    // Send to FollowUpBoss if widgetTracker is available
                    if (typeof window.widgetTracker !== 'undefined') {
                        const leadData = {
                            name: (firstName + ' ' + lastName).trim(),
                            email: email,
                            phone: phone,
                            message: message,
                            source: source,
                            property_url: window.location.href
                        };
                        
                        console.log('ð FollowUpBoss: Sending lead data:', leadData);
                        console.log('ð FollowUpBoss: widgetTracker available:', typeof window.widgetTracker);
                        
                        window.widgetTracker('send', 'lead', leadData);
                        
                        console.log('â FollowUpBoss: Lead sent successfully!');
                    } else {
                        console.warn('â ï¸ FollowUpBoss: widgetTracker not available. Make sure FollowUpBoss tracking is enabled.');
                    }
                    
                    // Reset form
                    form.reset();
                });
            });
        });
